		if (@zeny_bonus)
			dispbottom "Ao morrer você perde o bonus de zeny.";
		set @mob_kill_count, 0;
		set @zeny_bonus, 0;
		end;
	
OnPCLoadMapEvent:
	getmapxy(.@cur_map$,.@cur_x,.@cur_y,0);
	if (@last_map$ != .@cur_map$ || @loadevent_set == 0)
	{
		// Ao logar @loadevent_set zerá sempre zero, forçando a atualização do mapa atual.
		set @loadevent_set, 1;
		
		if (@zeny_bonus)
			dispbottom "Mudando de mapa você perde o bonus de zeny.";
		set @last_map$, .@cur_map$;
		set @mob_kill_count, 0;
		set @zeny_bonus, 0;
	}
	end;
