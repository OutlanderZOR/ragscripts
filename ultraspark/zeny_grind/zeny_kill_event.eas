			if (isloggedin(.@partyaid[.@i]))
				set .@split, .@split + 1;
			set .@i, .@i + 1;
		}
		set .@i, 0;
		while(.@i < .@size)
		{
			if (isloggedin(.@partyaid[.@i]))
			{
