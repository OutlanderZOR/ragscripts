//|===============================================================================================================
//|	Funções essenciais
//|===============================================================================================================
	function f_npctalk
	{
		set .@npcname$, getarg(0);
		set .@msg$, getarg(1);
		atcommand "@npctalk " + .@npcname$ + "," + .@msg$;
		return;
	}
	
	// Monta string de opção do menu
	function f_opt_n
	{
		set .@str$, getarg(0);
		return "^" + .color_n$ + "(~) ^000000" + .@str$ + "^" + .color_n$ + " (~)^000000";
	}
	
	function f_opt_w
	{
		set .@str$, getarg(0);
		return "^" + .color_w$ + "(*) ^000000" + .@str$ + "^" + .color_w$ + " (*)^000000";
	}
	
	function f_color
	{
		set .@str$, getarg(0);
		set .@color$, getarg(1);
		return "^"+.@color$+.@str$+"^000000";
	}
	
	function f_sex
	{
		set .@str_m$, getarg(0);
		set .@str_f$, getarg(1);
		return (Sex?.@str_m$:.@str_f$);
	}
	
	function f_destaque
	{
		set .@str$, getarg(0);
		return "^d80505" + .@str$ + "^000000";
	}
	
	function f_player_name
	{
		return "^23a0f7" + strcharinfo(0) + "^000000";
	}
	
	// Abstração do comando cutin
	// pos = 1 (esquerda)
	// pos = 2 (direita)
	function f_cutin
	{
		set .@cutin_str$, getarg(0);
		set .@cutin_pos, getarg(1);
		cutin .@cutin_str$, .@cutin_pos;
		return;
	}
	
	function f_cutin_sal_sad
