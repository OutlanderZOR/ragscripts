	set .title_go$, "[^"+.color_go$+"Garoto^000000]";
	
	// Variavel Garota
	set .color_ga$, "d500f5";
	set .title_ga$, "[^"+.color_ga$+"Garota^000000]";
	
	// Variaveis Salim
