	function f_warp_capital
	{
		warp "prontera",156,181;
		return;
	}
	
	function f_pular_intro
	{
		f_cutin_nyu_normal(2);
		mes .title_n$;
		mes "A etapa de introdução é muito importante!";
		mes " ";
		mes "Pulando a introdução você terá dificuldades para mudar de classe e subir de level.";
		next;
		mes .title_n$;
		mes "Deseja mesmo sair?";
		next;
		if (select(f_opt_n("Não") + ":" + f_opt_n("Sim"))-1)
		{
			f_cutin_nyu_feliz(2);
			mes .title_n$;
			mes "Bem, como quiser. ^^";
			mes " ";
			mes "Me chame caso tenha alguma dúvida, estarei sempre em prontera " +
				"para atender os jogadores.";
			close2;
			set intr_concluida, 1;
			return 1;
		}
		else
			return 0;
	}
	
