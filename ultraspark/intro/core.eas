	function f_set_skillpoint
	{
		resetskill;
		switch(Class)
		{
			case Job_Rune_Knight_T:
			case Job_Warlock_T:
			case Job_Ranger_T:
			case Job_Arch_Bishop_T:
			case Job_Mechanic_T:
			case Job_Guillotine_Cross_T:
			case Job_Royal_Guard_T:
			case Job_Sorcerer_T:
			case Job_Minstrel_T:
			case Job_Wanderer_T:
			case Job_Sura_T:
			case Job_Genetic_T:
			case Job_Shadow_Chaser_T:
				set skillpoint, 130;
				break;
			case Job_Kagerou:
			case Job_Oboro:
				set skillpoint, 60;
				break;
			case Job_SuperNovice:
				set skillpoint, 10;
				break;
			case Job_Gunslinger:
				set skillpoint, 10;
				break;
			case Job_Star_Gladiator:
				set skillpoint, 50;
				break;
			case Job_Soul_Linker:
				set skillpoint, 50;
				break;
		}
		return;
	}
	
	function f_obter_set
	{
		if (!intro_obter_set)
		{
			unequip(EQI_ARMOR);
			unequip(EQI_GARMENT);
			unequip(EQI_SHOES);
			
			// armor + peco peco card
			getitem2 2306,1,1,4,0,4031,0,0,0;
			// muffler + raydric card
			getitem2 2504,1,1,4,0,4133,0,0,0;
			// shoes + verit card
			getitem2 2404,1,1,4,0,4107,0,0,0;
			
			// equipa
			equip 2306;
			equip 2504;
			equip 2404;
			
			set intro_obter_set, 1;
		}
		return;
	}
	
	function f_get_platinum
	{
		skill 142,1,0;
		switch(BaseClass) 
		{
			case 0: if (Class !=23) skill 143,1,0; break;
			case 1: skill 144,1,0; skill 145,1,0; skill 146,1,0; break;
			case 2: skill 157,1,0; break;
			case 3: skill 147,1,0; skill 148,1,0; break;
			case 4: skill 156,1,0; break;
			case 5: skill 153,1,0; skill 154,1,0; skill 155,1,0; break;
			case 6: skill 149,1,0; skill 150,1,0; skill 151,1,0; skill 152,1,0; break;
			default: break; 
		}
		switch(roclass(eaclass()&EAJ_UPPERMASK)) 
		{
			case 7: skill 1001,1,0; break;
			case 8: skill 1014,1,0; break;
			case 9: skill 1006,1,0; break;
			case 10: skill 1012,1,0; skill 1013,1,0; break;
			case 11: skill 1009,1,0; break;
			case 12: skill 1003,1,0; skill 1004,1,0; break;
			case 14: skill 1002,1,0; break;
			case 15: skill 1015,1,0; skill 1016,1,0; break;
			case 16: skill 1007,1,0; skill 1008,1,0; skill 1017,1,0; skill 1018,1,0; skill 1019,1,0; break;
			case 17: skill 1005,1,0; break;
			case 18: skill 238,1,0; break;
			case 19: skill 1010,1,0; break;
			case 20: skill 1011,1,0; break;
			default: break; 
		}
		return;
	}
	
OnNPCKillEvent:
	if (killedrid == 1002 && BaseLevel <= 90)
	{
		getmapxy(.@map$,.@x,.@y,0);
		if (.@map$ == "rot_intro3")
		{
			if (BaseLevel < 90)
				getexp 200,1200;
			else if (!@aviso_level_90)
			{
				set @aviso_level_90, 1;
				announce "Após atingir o level 90 os Porings passam a valer menos experiência.",bc_self;
				sleep2 10000;
				announce "Fale com a Nyuang para concluir esta etapa da introdução.",bc_self;
				viewpoint 1,118,110,1,0xFF0000;
			}
		}
	}
	end;

//|===============================================================================================================
//|	Primeira introdução (que_hugel)
//|===============================================================================================================
OnNyuIntro1:
	// variavel para controle de progresso
	set intr_concluida, 0;
	set @ignore_ontouch, 0;
	f_cutin_nyu_feliz(2);
	mes .title_n$;
	mes "Olá " + f_player_name();
	if (!#intr_old_user)
	{
		mes "Bem vindo(a) ao RO Tactics!";
		mes " ";
		mes "Eu sou ^" + .color_n$ + "Nyuang^000000, guia oficial do " + f_destaque("ROT") + ".";
	}
	else
		mes "Novo personagem né, espero que esteja gostando do ROT.";
	next;
L_Pegunta_Classe:
	f_cutin_nyu_feliz(2);
	mes .title_n$;
	mes "Entao...";
	mes "Vamos escolher sua classe?";
	next;
	menu f_opt_n("Vamos"),-,f_opt_n("Não, espere!"),L_Pular_Intro;
	warp "rot_intro2",104,102;
	end;
	
L_Pular_Intro:
	f_cutin_nyu_normal(2);;
	mes .title_n$;
	mes "O que houve?";
	next;
	menu f_opt_n("Nada não"),L_Pegunta_Classe,f_opt_n("Quero pular a introdução"),-;
	if (f_pular_intro())
		f_warp_capital();
	else
		goto L_Pegunta_Classe;
	end;
	
//|===============================================================================================================
//|	Nyuang jobchange evento OnTouch
//|===============================================================================================================
OnNyuIntro2:
	f_cutin_nyu_feliz(2);
	mes .title_n$;
	mes "Aqui temos os mestres das principais artes de combate.";
	mes " ";
	mes "Para facilitar, todos eles oferecem um treinamento expresso, mudança de classe imediata garantida!";
	next;
	f_cutin_nyu_tease(2);
	mes .title_n$;
	mes "Nessa arena você poderá trocar de classe quantas vezes quiser, "+
		"mas depois que sairmos a escolha será permanente.";
	next;
	f_cutin_nyu_feliz(2);
	mes .title_n$;
	mes "Então tome seu tempo e escolha com calma!";
	mes " ";
	mes "Quando tiver decidido me avise!";
	close2;
	cutin "",255;
	// Define estado de leitura ontouch
	set @ignore_ontouch, 1;
	end;	
	
//|===============================================================================================================
//|	Nyuang jobchange dialogo pós seleção
//|===============================================================================================================
OnNyuIntro3:
	mes .title_n$;
	if (!Class)
	{
		f_cutin_nyu_normal(2);;
		mes "Está tendo alguma dificuldade?";
		mes "Para trocar de classe basta conversar com os NPCs mestres.";
		mes " ";
		mes "Posso ajudar em mais alguma coisa?";
		next;
		if (select(f_opt_n("Nada, vou escolher minha classe") + ":" + f_opt_n("Quero sair daqui"))-1)
		{
			if (f_pular_intro())
				f_warp_capital();
		}
		else
		{
			f_cutin_nyu_feliz(2);
			mes .title_n$;
			mes "Ok, estarei esperando! ~";
			
		}
		close2;
	}
	else
	{
		f_cutin_nyu_feliz(2);
		mes "Muito bem, agora você precisa treinar um pouco.";
		mes " ";
		mes "Eu conheço um mapa muito bom, você subirá de nivel em um piscar de olhos.";
		next;
		switch(select(f_opt_n("Vamos") + ":" + f_opt_n("Espere, ainda não decidi") + ":" + f_opt_n("Quero sair da introdução")))
		{
			case 1:
				// Confirma estado para ativação do OnTouch na proxima etapa
				set @ignore_ontouch, 0;
				f_get_platinum();
				atcommand "@blvl 10";
				atcommand "@jlvl 10";
				f_set_skillpoint();
				percentheal 100,100;
				save "rot_intro3",118,106;
				warp "rot_intro3",118,106;
				f_obter_armas();
				break;
			case 2:
				f_cutin_nyu_tease(2);
				mes .title_n$;
				mes "Ok.. ok.";
				mes "Estou esperando.";
				break;
			case 3:
				if (f_pular_intro())
				{
					f_set_skillpoint();
					f_get_platinum();
					f_warp_capital();
				}
				break;
		}
		close2;
	}
	cutin "",255;
	end;
	
//|===============================================================================================================
//|	Nyuang treinamento poring OnTouch
//|===============================================================================================================
OnNyuIntro4:
	f_cutin_nyu_feliz(2);
	mes .title_n$;
	mes "Este é o mapa secreto dos Super Porings.";
	mes " ";
	mes "Aqui um Poring vale muita "+ f_destaque("exp") +", mate quantos forem necessários "+
		"para chegar ao " + f_destaque("level 90") + ".";
	next;
	mes .title_n$;
	mes "Fale comigo quando terminar ou quando achar que matou o suficiente.";
	mes " ";
	mes "Estarei esperando. Ok?";
	close2;
	set @ignore_ontouch, 1;
	cutin "",255;
	end;
	

	
//|===============================================================================================================
//|	Nyuang treinamento poring on click
//|===============================================================================================================
OnNyuIntro5:
	mes .title_n$;
	if (BaseLevel < 90)
	{
		f_cutin_nyu_normal(2);
		mes "Já terminou?";
		mes " ";
		mes "Acho que seu level ainda é baixo, recomendo que você treine "+
			"aqui pelo menos até o " + f_destaque("level 90");
		next;
		switch(select(f_opt_n("Hmm Ok, vou upar mais") + ":" + f_opt_n("Acho que já é o suficiente") + ":" + f_opt_n("Quero pular a introdução")))
		{
			case 1:
				f_cutin_nyu_feliz(2);
				mes .title_n$;
				mes "Certo, estarei esperando.";
				close2;
				break;
			case 2:
				f_cutin_nyu_feliz(2);
				mes .title_n$;
				mes "Ok.. ok.";
				mes "Vamos seguir em frente para a proxima etapa então?";
				next;
				if (select(f_opt_n("Vamos") + ":" + f_opt_n("Na verdade, mudei de idéia"))-1)
				{
					f_cutin_nyu_tease(2);
					mes .title_n$;
					mes "Heh.. me chame quando decidir.";
					close2;
				}
				else
				{
					set @ignore_ontouch, 0;
					warp "market",100,82;
				}
				break;
			case 3:
				if (f_pular_intro())
					f_warp_capital();
				close2;
		}
	}
	else
	{
		f_cutin_nyu_feliz(2);
		mes "Parece que você já está " + f_sex("pronto","pronta") + ".";
		mes " ";
		mes "Agora vamos para o mercado. O mercador chefe é meu amigo, se eu pedir ele vai te ajudar com certeza!";
		next;
		switch(select(f_opt_n("Sim, vamos") + ":" + f_opt_n("Ainda não, espere") + ":" + f_opt_n("Quero pular a introdução")))
		{
			case 1:
				set @ignore_ontouch, 0;
				warp "market",100,82;
				break;
			case 2:
				f_cutin_nyu_tease(2);
				mes .title_n$;
				mes "Ok.. ok.";
				mes "Me avise quando estiver pronto.";
				close2;
				break;
			case 3:
				if (f_pular_intro())
					f_warp_capital();
				close2;
				break;
		}
	}
	cutin "",255;
	end;
	
//|===============================================================================================================
//|	Wayne treinamento poring on click
//|===============================================================================================================
OnWayne:
	f_cutin_way(2);
	mes .title_w$;
	mes "Olá " + f_player_name() + ", tudo bem?";
	next;
	if(select(f_opt_w("Tudo certo") + ":" + f_opt_w("Quem diabos é você?"))-1)
	{
		mes .title_w$;
		mes "Meu nome é Wayne, sou supervisor chefe dos NPCs do " + f_destaque("ROT") + ".";
		mes " ";
		mes "Estou aqui para observar o desempenho da "+f_color("Nyuang",.color_n$)+".";
		next;
		mes .title_w$;
		mes "O GM contratou ela recentemente para cuidar dos nossos novos jogadores.";
		mes " ";
		mes "Por isso tenho que ficar de olho!";
		next;
		if (!(select(f_opt_w("Ta certo") + ":" + f_opt_w("Ok, quero treinar agora"))-1))
		{
			mes .title_w$;
			mes "Não tenho nada contra estagiários, mas as vezes a "+f_color("Nyuang",.color_n$)+" se empolga de mais, isso me preocupa.";
			mes " ";
			mes "Devemos ser sérios e profissionais durante o trabalho!";
			next;
			mes .title_w$;
			mes "Falando nisso, como você avalia o desempenho dela?";
			next;
			set .nota_nyuang, select(f_opt_w("Muito bom") + ":" + f_opt_w("Bom") + ":" + f_opt_w("Razoável") + ":" + f_opt_w("Ruim"));
			mes .title_w$;
			mes "Obrigado pelo seu tempo, não quero atrapalhar mais, pode ir treinar.";
			set @mute_wayne, 1;
			close2;
			donpcevent "Intro_Core::OnAvaliacaoNyu";
		}
		else
		{
			mes .title_w$;
			mes "Certo, não quero atrapalhar.";
			close2;
		}
	}
	else
		close2;
	cutin "",255;
	end;
	
OnAvaliacaoNyu:
	if (!.dialogo_aval && .nota_nyuang >= 1 && .nota_nyuang <= 4)
	{
		sleep 2000;
		set .dialogo_aval, 1;
		set .@nyu_npc$, "Nyuang#i3";
		set .@way_npc$, "Supervisor Wayne#i1";
		switch(.nota_nyuang)
		{
			// Muito bom
			case 1:
				f_npctalk(.@way_npc$, "Muito bem Nyuang, estamos recebendo elogios.");
				sleep 5000;
				f_npctalk(.@nyu_npc$, "Estou me esforçando! ^_~");
				break;
			// Bom
			case 2:
				f_npctalk(.@way_npc$, "Parece que você está indo bem Nyuang.");
				sleep 3000;
				f_npctalk(.@nyu_npc$, "Que bom! ^^");
				sleep 5000;
				f_npctalk(.@way_npc$, "O que aparenta não importa, eu quero resultados!");
				sleep 4000;
				f_npctalk(.@nyu_npc$, "As vezes vc é um chato sabia? <_<");
				break;
			// Razoável
			case 3:
				f_npctalk(.@way_npc$, "Nyuang, você sabe que pra mim só existem profissionais bons e ruins.");
				sleep 3000;
				f_npctalk(.@nyu_npc$, "Huh, o que vc ta resmungando ai?");
				sleep 5000;
				f_npctalk(.@way_npc$, "Seu desempenho está baixo, desse jeito teremos que reduzir o seu salário!");
				sleep 3000;
				f_npctalk(.@nyu_npc$, "Mas eu já ganho uma mixaria!");
				sleep 2800;
				f_npctalk(.@way_npc$, "Não ta facil pra ninguém.");
				break;
			// Ruim
			case 4:
				f_npctalk(.@way_npc$, "Nyuang, seu desempenho é inadmissível, você está demitida!");
				sleep 5000;
				f_npctalk(.@nyu_npc$, "Você não pode me demitir, foi o GM em pessoa que me contratou!");
				sleep 5000;
				f_npctalk(.@way_npc$, "Verdade. Mas fique sabendo que falarei mal ao seu respeito na proxima reunião!");
				sleep 3000;
				f_npctalk(.@nyu_npc$, "Você é um desgraçado sabia? <_<");
				break;
		}
		sleep 60 * 1000;
		set .dialogo_aval, 0;
	}
	end;
	
